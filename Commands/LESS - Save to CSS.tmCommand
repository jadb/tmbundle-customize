	<string>LESS - Save to CSS</string>
	<key>outputCaret</key>
	<string>afterOutput</string>
	<key>outputFormat</key>
	<string>text</string>
	<key>outputLocation</key>
	<string>toolTip</string>
	<key>scope</key>
	<string>source.css.less</string>
	<key>uuid</key>
	<string>1E6E7B98-8AC8-4C3B-9146-8CE38DA2BC0D</string>
	<key>version</key>
	<integer>2</integer>
</dict>
