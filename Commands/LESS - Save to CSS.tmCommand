<plist version="1.0">
<dict>
	<key>beforeRunningCommand</key>
	<string>saveActiveFile</string>
	<key>command</key>
	<string>#!/usr/bin/env ruby
file = ENV["TM_FILEPATH"]
name = ENV["TM_FILENAME"].chomp(File.extname(ENV["TM_FILENAME"]))
dir = File.dirname(ENV["TM_FILEPATH"]).chomp("less")
system("lessc --verbose \"#{file}\" \"#{dir}/#{name}.css\"")</string>
	<key>input</key>
	<string>document</string>
	<key>inputFormat</key>
	<string>text</string>
	<key>keyEquivalent</key>
	<string>@S</string>
